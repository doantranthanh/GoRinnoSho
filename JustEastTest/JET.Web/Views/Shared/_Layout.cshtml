@{
    Layout = null;
}

<!DOCTYPE html>

<html html lang="en" class="no-js" ng-app="justEastTesting">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>_Layout</title>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.9/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.9/angular-route.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.9/angular-sanitize.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.9/angular-messages.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.9/angular-cookies.js"></script>
    <script src="https://cdn.jsdelivr.net/ngstorage/0.3.6/ngStorage.min.js"></script>
</head>
<body>
    <!--main body -->
    <main id="main" role="main" class="main">
        @RenderBody()
    </main>
    @Scripts.Render("~/bundles/application")
    @Scripts.Render("~/bundles/spa")
    @Scripts.Render("~/bundles/helpers")
</body>
</html>

@RenderSection("masterscript", false)
